# AIOS v2 Staging Environment Configuration

# Environment Settings
ENVIRONMENT=staging
DEBUG=false
LOG_LEVEL=INFO

# Security Keys (Use secure keys from secrets manager)
SECRET_KEY=${STAGING_SECRET_KEY}
JWT_SECRET_KEY=${STAGING_JWT_SECRET}
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=1440

# Database Configuration
DATABASE_URL=${STAGING_DATABASE_URL}
DATABASE_POOL_SIZE=10
DATABASE_MAX_OVERFLOW=20

# Redis Configuration
REDIS_URL=${STAGING_REDIS_URL}
REDIS_POOL_SIZE=20

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=8
CORS_ORIGINS=https://staging.aios.example.com

# Frontend Configuration
NEXT_PUBLIC_API_URL=https://staging-api.aios.example.com
NEXT_PUBLIC_WS_URL=wss://staging-api.aios.example.com

# Feature Flags
FEATURE_MARKETPLACE_ENABLED=true
FEATURE_AGENT_BUILDER_ENABLED=true
FEATURE_COLLABORATION_ENABLED=true
FEATURE_ANALYTICS_ENABLED=true

# Monitoring and Analytics
SENTRY_DSN=${STAGING_SENTRY_DSN}
PROMETHEUS_ENABLED=true
PROMETHEUS_PORT=9090

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_DEFAULT=200
RATE_LIMIT_BURST=50

# Performance Settings
CACHE_TTL=7200
QUERY_TIMEOUT=60
REQUEST_TIMEOUT=120

# Development Settings
HOT_RELOAD=false
MOCK_EXTERNAL_SERVICES=false